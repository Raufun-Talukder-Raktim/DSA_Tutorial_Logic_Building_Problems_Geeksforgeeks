// Online C compiler to run C program online
#include <stdio.h>
void print_binary(int n){
    if (n==0){
         printf("0");
         //return 0;
    }
    int index =0;
    int binary[16];
    while(n>0){
        binary[index] = n%2 ;
        n = n/2;
        index++;
    }
    for (int i = index-1;i>=0;i--){
        printf("%d", binary[i]);
    }
}


int main() {
    int n = 310;
    print_binary(n);
    return 0;
}
